<?php
/**
 * @file
 * openacademy_events_demo.install
 */

/**
 * Implements hook_enable().
 */
function openacademy_events_demo_enable() {
  _panopoly_core_import_demo_content(array(
    'OpenacademyEventsDemoTerm',
    'OpenacademyEventsDemoNode',
  ));
}

/**
 * Implements hook_disable().
 */
function openacademy_events_demo_disable() {
  _panopoly_core_rollback_demo_content(array(
    'OpenacademyEventsDemoNode',
    'OpenacademyEventsDemoTerm',
  ));
}

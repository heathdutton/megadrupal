<?php
/**
 * @file
 * The installer file for Milli Piyango module.
 */

/**
 * Implementation of hook_uninstall().
 */
function milli_piyango_uninstall() {
  variable_del('milli_piyango_sayisal');
  variable_del('milli_piyango_sanstopu');
  variable_del('milli_piyango_onnumara');
  variable_del('milli_piyango_superloto');
  db_query("DELETE FROM {block} WHERE module = 'milli_piyango'");
}
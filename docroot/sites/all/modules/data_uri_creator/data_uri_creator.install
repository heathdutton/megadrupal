<?php
/**
 * @file
 * Installation file for the Data URI Creator module.
 */

/**
 * Implements hook_uninstall().
 */
function data_uri_creator_uninstall() {
  // Remove the variables that store the settings values.
  variable_del('data_uri_creator_max_file_size');
  variable_del('data_uri_creator_page_public');
}

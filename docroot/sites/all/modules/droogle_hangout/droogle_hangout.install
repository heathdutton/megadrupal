<?php

/**
 * @file
 * Provides uninstall on variables.
 */

/**
 * Implements hook_uninstall().
 */
function droogle_hangout_uninstall() {
  // Delete variables.
  variable_del('droogle_hangout_clientid');
  variable_del('droogle_hangout_client_secret');
  variable_del('droogle_hangout_refresh_token');
  variable_del('droogle_hangout_custom_css');
}

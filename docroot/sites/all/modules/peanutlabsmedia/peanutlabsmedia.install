<?php

/**
 * @file
 * Install, update, and uninstall functions for peanutlabsmedia.
 *
 * @ingroup peanutlabsmedia
 */



/**
 * Implements hook_uninstall().
 */
function peanutlabsmedia_uninstall() {
  variable_del('peanutlabsmedia_userid');
  variable_del('peanutlabsmedia_appkey');
  variable_del('peanutlabsmedia_frame_height');
  variable_del('peanutlabsmedia_frame_width');
}

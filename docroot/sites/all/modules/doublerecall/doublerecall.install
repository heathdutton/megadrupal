<?php

/**
 * @files
 * Install, uninstall and update functions.
 */

/**
 * Implements hook_uninstall().
 */
function doublerecall_uninstall() {
  variable_del('doublerecall_server');
  variable_del('doublerecall_publisher_token');
  variable_del('doublerecall_hide_node');
  variable_del('doublerecall_callback_served');
  variable_del('doublerecall_callback_retyped');
  variable_del('doublerecall_callback_paid');
  variable_del('doublerecall_callback_noad');
}
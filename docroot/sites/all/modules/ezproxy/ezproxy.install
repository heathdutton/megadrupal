<?php

/**
 * @file
 * Install EZProzy module
 */
 
/**
 * Implementation of hook_install()
 */
function ezproxy_install() {
  drupal_set_message(st('EZProxy module installed'));
}

/**
 * Implementation of hook_uninstall()
 */
function ezproxy_uninstall() {
  db_query("DELETE FROM {variable} WHERE name LIKE :ezproxy%", array(':ezproxy' => 'ezproxy'));
  drupal_set_message(st('EZProxy module uninstalled'));
}

<ul class="recommended">
  {{#items}}
  <li class="recommended__item">
    <a href="{{url}}">
      <img class="recommended__thumb" src="{{thumbnail}}">
      <span class="recommended__title">{{ title }}</span>
      {{#bundle}}<span class="recommended__bundle">{{ bundle }}</span>{{/bundle}}
    </a>
  </li>
  {{/items}}
</ul>

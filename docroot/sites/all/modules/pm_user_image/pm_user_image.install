<?php

/**
 * @file
 * Install, update, and uninstall functions for the PM USer Image module.
 */

/**
 * Implements hook_uninstall(). 
 */
function pm_user_image_uninstall() {
  variable_del('pm_user_image_height');
  variable_del('pm_user_image_width');
  variable_del('pm_user_image_path_type');
  variable_del('pm_user_image_default_external');
  variable_del('pm_user_image_default_internal');
  variable_del('pm_user_image_image_style');
}

<?php
/**
 * @file
 * Install, update, and uninstall functions for the taxonomy_protect module.
 */

/**
 * Implements hook_install().
 */
function taxonomy_protect_install() {
  variable_set('taxonomy_protect_vocabularies', array());
}

/**
 * Implements hook_uninstall().
 */
function taxonomy_protect_uninstall() {
  variable_del('taxonomy_protect_vocabularies');
}


<?php
/**
 * @file
 * The install file for A12 Connect
 */

/**
 * Implements hook_uninstall().
 *
 * Tidy up after ourselves, by deleting all variables we have created.
 */
function a12_connect_uninstall() {
  variable_del('a12_identifier');
  variable_del('a12_key');
  variable_del('a12_network_address');
  variable_del('a12_subscription_data');
}

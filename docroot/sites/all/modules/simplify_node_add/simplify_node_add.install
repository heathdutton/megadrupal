<?php
/**
 * @file
 * Install, update and uninstall functions for the simplify_node_add module.
 */

/**
 * Implementation of hook_uninstall().
 */
function simplify_node_add_uninstall() {
  db_delete('variable')
  ->condition('name', '%s%')
  ->execute();
  cache_clear_all('variables', 'cache');
}

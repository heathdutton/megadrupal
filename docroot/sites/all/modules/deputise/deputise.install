<?php
/**
 * @file
 * Install, update and uninstall functions for the Deputise module.
 */

/**
 * Implements hook_install().
 */
function deputise_install() {
  // nothing to do
}

/**
 * Implements hook_uninstall().
 */
function deputise_uninstall() {
  // Unset variables
  $variables = array('deputise_modules_mode',
                     'deputise_module_list',
                     );
  foreach ($variables as $variable) {
    variable_del($variable);
  }
}

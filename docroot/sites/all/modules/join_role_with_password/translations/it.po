#: join_role_with_password.module:14
msgid "Administer join_role_with_password"
msgstr "Amministra join_role_with_password"

#: join_role_with_password.module:17
msgid "May join a role using a password"
msgstr "Pu&ograve; unirsi a un ruolo usando una password"

#: join_role_with_password.module:125
msgid "Add password to an existing role"
msgstr "Associa una password ad un ruolo esistente"

#: join_role_with_password.module:142;293;425;559;589;865;988
msgid "Error from the query!"
msgstr "Errore dalla query!"

#: join_role_with_password.module:147
msgid "There is no role you can add a password!"
msgstr "Non c'&egrave; alcun ruolo al quale puoi associare una password!"

#: join_role_with_password.module:178
msgid "Add a password to this role"
msgstr "Associa una password a questo ruolo"

#: join_role_with_password.module:180
msgid "Authorized users will be able to join this role using a password"
msgstr "Gli utenti autorizzati saranno in grado di unirsi a questo ruolo usando una password"

#: join_role_with_password.module:186;661
msgid "Insert a password"
msgstr "Inserisci una password"

#: join_role_with_password.module:187;199
msgid "Give a password to this role"
msgstr "Dai una password a questo ruolo"

#: join_role_with_password.module:192;193
msgid "Repeat the password"
msgstr "Ripeti la password"

#: join_role_with_password.module:224;518
msgid "Passwords mismatch"
msgstr "Le password non coincidono"

#: join_role_with_password.module:228;522;786
msgid "Invalid password"
msgstr "Password invalida"

#: join_role_with_password.module:264
msgid "Password added to role: %role"
msgstr "Password associata al ruolo: %role"

#: join_role_with_password.module:267;352;399;566;581;770
msgid "Access denied"
msgstr "Access negato"

#: join_role_with_password.module:283
msgid "Remove password from a role"
msgstr "Rimuovi la password da un ruolo"

#: join_role_with_password.module:297;429;593
msgid "List of roles with password is empty"
msgstr "La lista di ruoli con password &egrave; vuota"

#: join_role_with_password.module:316;335
msgid "Remove password from this role"
msgstr "Rimuovi la password da questo ruolo"

#: join_role_with_password.module:318
msgid "If you remove password from a role, users will be no more able to join it using a password"
msgstr "Se rimuovi una password da un ruolo, gli utenti non saranno pi&ugrave; in grado di unirsi al ruolo usando una password"

#: join_role_with_password.module:332
msgid "Remove role password"
msgstr "Rimuovi la password del ruolo"

#: join_role_with_password.module:334;491
msgid "This action cannot be undone."
msgstr "Quest'azione non pu&ograve; essere annullata"

#: join_role_with_password.module:336;492
msgid "Cancel"
msgstr "Annulla"

#: join_role_with_password.module:359
msgid "Invalid role id"
msgstr "Id del ruolo non valido"

#: join_role_with_password.module:365;805
msgid "Severe error: role is not in the table"
msgstr "Errore grave: il ruolo non &egrave; nella tabella"

#: join_role_with_password.module:392
msgid "Removed password from role: %role"
msgstr "Password rimossa dal ruolo: %role"

#: join_role_with_password.module:395
msgid "Error from query!"
msgstr "Errore dalla query!"

#: join_role_with_password.module:416
msgid "Change password for a role"
msgstr "Cambia la password di un ruolo"

#: join_role_with_password.module:450;492
msgid "Change password to this role"
msgstr "Cambla la password di questo ruolo"

#: join_role_with_password.module:452
msgid "Choose what role to change the password"
msgstr "Scegli il ruolo al quale cambiare la password"

#: join_role_with_password.module:467
msgid "New password"
msgstr "Nuova password"

#: join_role_with_password.module:469
msgid "You may enter a new password for this role"
msgstr "Puoi inserire una nuova password per questo ruolo"

#: join_role_with_password.module:473
msgid "New password again"
msgstr "Di nuovo la nuova password"

#: join_role_with_password.module:475
msgid "Enter new password again"
msgstr "Inserisci ancora la nuova password"

#: join_role_with_password.module:490
msgid "Change role password"
msgstr "Cambia la password del ruolo"

#: join_role_with_password.module:562
msgid "Role %role updated - password changed!"
msgstr "Ruolo %role aggiornato - password cambiata!"

#: join_role_with_password.module:644
msgid "Join a role"
msgstr "Unisciti a un ruolo"

#: join_role_with_password.module:653;667
msgid "Join this role"
msgstr "Unisciti a questo ruolo"

#: join_role_with_password.module:655
msgid "You can join one of these roles if you know the correct password"
msgstr "Puoi unirti ad uno di questi ruoli se conosci la password corretta"

#: join_role_with_password.module:662
msgid "Enter the correct password to join this role"
msgstr "Inserisci la password corretta per unirti a questo ruolo"

#: join_role_with_password.module:672
msgid "There is no role that you can join"
msgstr "Non c'&egrave; alcun ruolo al quale puoi unirti"

#: join_role_with_password.module:685
msgid "Leave a role"
msgstr "Abbandona un ruolo"

#: join_role_with_password.module:693;709;724
msgid "Leave this role"
msgstr "Abbandona questo ruolo"

#: join_role_with_password.module:695
msgid "You can leave one of this roles; you dont need to know the password to leave it"
msgstr "Puoi abbandonare uno di questi ruoli; non hai bisogno di conoscerne la password per abbandonarlo"

#: join_role_with_password.module:713
msgid "There is no role that you can leave"
msgstr "Non c'&egrave; alcun ruolo che tu possa lasciare"

#: join_role_with_password.module:744
msgid "Do it now"
msgstr "Fallo subito"

#: join_role_with_password.module:782
msgid "Missing password"
msgstr "Password mancante"

#: join_role_with_password.module:797;847
msgid "Sorry, that role does no more exists"
msgstr "Spiacente, quel ruolo non esiste pi&ugrave;"

#: join_role_with_password.module:825
msgid "You have joined the role: %role_name "
msgstr "Ti sei unito al ruolo: %role_name"

#: join_role_with_password.module:828
msgid "Invalid password for role: %role_name"
msgstr "Password sbagliata per il ruolo: %role_name"

#: join_role_with_password.module:862
msgid "You have left role: %role"
msgstr "Hai abbandonato il ruolo: %role"

#: join_role_with_password.module:888;919
msgid "Invalid call - $role_id does not exist"
msgstr "Chiamata invalid - $role_id non esiste"

#: join_role_with_password.module:952
msgid "Password too short"
msgstr "Password troppo corta"

#: join_role_with_password.module:957
msgid "Password is blank or empty"
msgstr "La password &egrave; vuota o formata solo da spazi"

#: join_role_with_password.module:1011
msgid "Database cleanup: role %removed has been removed."
msgstr "Pulito il database: il ruolo %removed &egrave; stato rimosso."

#: join_role_with_password.module:1028
msgid "Manage the leave role section"
msgstr "Gestisci la sezione per lasciare i ruoli"

#: join_role_with_password.module:1029
msgid "If disabled, users will not be able to leave a role; they can only join."
msgstr "Se disabilitata, gli utenti non potranno lasciare ruoli; potranno solo unirsi."

#: join_role_with_password.module:1032
msgid "disabled"
msgstr "disabilitata"

#: join_role_with_password.module:1033
msgid "enabled"
msgstr "abilitata"

#: join_role_with_password.module:1046
msgid "Enforce minimum password length"
msgstr "Definisci una lunghezza minima per la password"

#: join_role_with_password.module:1047
msgid "Zero means no minimum length"
msgstr "Zero significa nessuna lunghezza minima"

#: join_role_with_password.module:30;31
msgid "Manage role passwords"
msgstr "Gestisci le password dei ruoli"

#: join_role_with_password.module:39
msgid "Add role password"
msgstr "Associa password ai ruoli"

#: join_role_with_password.module:49
msgid "Remove role passwords"
msgstr "Rimuovi password dai ruoli"

#: join_role_with_password.module:59
msgid "Change role passwords"
msgstr "Cambia password ai ruoli"

#: join_role_with_password.module:69
msgid "Settings"
msgstr "Opzioni"

#: join_role_with_password.module:79
msgid "Join or leave a role"
msgstr "Unisciti o abbandona un ruolo"

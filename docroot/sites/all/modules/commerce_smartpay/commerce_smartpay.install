<?php

/**
 * @file
 * Deletes variables set by the user when the module is uninstalled.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_smartpay_uninstall() {

  // Commerce Smartpay settings.
  variable_del('commerce_smartpay_merchant_account');
  variable_del('commerce_smartpay_skin_code');
  variable_del('commerce_smartpay_shared_secret_test');
  variable_del('commerce_smartpay_shared_secret_live');
  variable_del('commerce_smartpay_transaction_mode');
}

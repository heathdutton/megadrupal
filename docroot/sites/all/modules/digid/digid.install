<?php
/**
 * @file
 * Provides installation details for DigiD.
 */

/**
 * Implementation of hook_uninstall().
 */
function digid_uninstall() {
  variable_del('digid_a-select_server');
  variable_del('digid_a-select_server_id');
  variable_del('digid_app_id');
  variable_del('digid_shared_secret');
  variable_del('digid_rid');
}

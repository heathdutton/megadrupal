<?php

/**
 * @file
 * Field validation plugins for Australian numbers, such as ABN, ACN and TFN.
 */

/**
 * Implements hook_ctools_plugin_directory().
 */
function fv_au_ctools_plugin_directory($module, $plugin) {
  if ($module == 'field_validation' && $plugin == 'validator') {
    return 'plugins/' . $plugin;
  }
}

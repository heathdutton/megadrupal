<?php
/**
 * @file
 * Install and uninstall hooks for Noindex Block module.
 */

/**
 * Implements hook_install().
 */
function noindex_block_uninstall() {
  // Delete module variables and clear variable cache.
  db_delete('variable')
    ->condition('name', 'noindex_block_%', 'LIKE')
    ->execute();
  cache_clear_all('variables', 'cache_bootstrap');
}

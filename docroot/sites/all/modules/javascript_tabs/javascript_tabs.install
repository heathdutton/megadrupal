<?php
/**
 * @file
 * Install, update, and uninstall functions for the javascript_tabs module.
 */

/**
 * Implements hook_uninstall().
 */
function javascript_tabs_uninstall() {
  variable_del('javascript_tabs_theme_visibility');
  variable_del('javascript_tabs_visibility');
  variable_del('javascript_tabs_pages');
}

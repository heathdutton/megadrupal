<?php
/**
 * @file
 * Uninstall functions for the going down module.
 */

/**
 * Implements hook_uninstall().
 */
function going_down_uninstall() {
  variable_del('going_down_warning');
  variable_del('going_down_action');
  variable_del('going_down_date');
  variable_del('going_down_hour');
  variable_del('going_down_minute');
  variable_del('going_down_time_format');
  variable_del('going_down_message');
  variable_del('going_down_style');
}

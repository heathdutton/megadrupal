<?php

/**
 * Defines the National Bank of Romania (BNR.ro) as currency exchange rate sync provider.
 *
 * @return array
 *   An array of information about the callback.
 *   The array contains a sub-array for each callback, with a machine name
 *   as the key.
 */
function commerce_multicurrency_ron_commerce_multicurrency_exchange_rate_sync_provider_info() {
  return array(
    'bnr' => array(
      'title' => t('National Bank of Romania (BNR)'),
      'callback' => 'commerce_multicurrency_ron_exchange_rate_sync_provider_bnr',
      'file' => drupal_get_path('module', 'commerce_multicurrency_ron') . '/commerce_multicurrency_ron.bnr.inc',
    ),
  );
}
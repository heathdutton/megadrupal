<?php
/**
 * @file
 * Install and uninstall functions for the Ime module.
 */

/**
 * Implements hook_install().
 */
function ime_install() {
  variable_set('ime_enable', 0);
}

/**
 * Implements hook_uninstall().
 */
function ime_uninstall() {
  variable_del('ime_enable');
  variable_del('ime_ids');
  variable_del('ime_pages');
}

<?php
/**
 * @file
 * openacademy_news_demo.install
 */

/**
 * Implements hook_enable().
 */
function openacademy_news_demo_enable() {
  _panopoly_core_import_demo_content(array(
    'OpenacademyNewsDemoTerm',
    'OpenacademyNewsDemoNode',
  ));
}

/**
 * Implements hook_disable().
 */
function openacademy_news_demo_disable() {
  _panopoly_core_rollback_demo_content(array(
    'OpenacademyNewsDemoNode',
    'OpenacademyNewsDemoTerm',
  ));
}

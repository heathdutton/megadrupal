<?php
/**
 * @file ajax_timeline.install
 */

/**
 * Implements hook_install().
 */
function ajax_timeline_install(){
}

/**
 * Implements hook_uninstall().
 */
function ajax_timeline_uninstall() {
  // Delete all module variables and clear the variable cache.
  db_delete('variable')
    ->condition('name', db_like('ajax_timeline_') . '%', 'LIKE')
    ->execute();
  cache_clear_all('variables', 'cache');
}
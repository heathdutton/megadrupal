<?php

/**
 * Implements hook_cron_queue_info().
 */
function gearman_test_cron_queue_info() {
  return array(
    'gearman_test_cron_queue' => array(
      'worker callback' => 'gearman_test_cron_queue_worker',
    ),
  );
}

/**
 * Worker function for our cron queue.
 */
function gearman_test_cron_queue_worker($data) {
  variable_set('gearman_test_cron_queue', $data);
}
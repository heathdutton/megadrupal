<?php

/**
 * @file
 * Installation file for the Multi Submit module.
 *
 * @ingroup multi_submit
 * @{
 */

/**
 * Implementation of hook_uninstall().
 */
function multi_submit_uninstall() {
  db_query("DELETE FROM {variable} WHERE name LIKE 'multi_submit_%'");
  cache_clear_all('variables', 'cache');
}

/**
 * @} End of "ingroup multi_submit".
 */

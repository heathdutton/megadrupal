<?php
/**
 * @file
 * Install, update and uninstall functions for the poll_chart module.
 */

/**
 * Implements hook_uninstall().
 */
function poll_chart_uninstall() {
  variable_del('poll_chart_type');
  variable_del('poll_chart_title');
  variable_del('poll_chart_latest');
  variable_del('poll_chart_width');
  variable_del('poll_chart_height');
}
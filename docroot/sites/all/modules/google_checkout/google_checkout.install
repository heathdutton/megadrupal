/**
 * Implements hook_uninstall().
 */
function google_checkout_uninstall() {
  variable_del('google_checkout_merchant_id');
  variable_del('google_checkout_merchant_key');
  variable_del('google_checkout_server_type');
  variable_del('google_checkout_currency');
}

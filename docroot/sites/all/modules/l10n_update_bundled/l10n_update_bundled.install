<?php
/**
 * @file
 * Install, update and uninstall hooks for the Localization update bundled module.
 */

/**
 * Implements hook_install().
 */
function l10n_update_bundled_install() {
  variable_set('l10n_update_bundled_key', drupal_random_key());
}

/**
 * Implements hook_uninstall().
 */
function l10n_update_bundled_uninstall() {
  variable_del('l10n_update_bundled_base_url');
  variable_del('l10n_update_bundled_key');
}

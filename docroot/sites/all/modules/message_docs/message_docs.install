<?php
function message_docs_install() {
  variable_set('message_docs_site_id',message_docs_generate_random_hash(12));
  variable_set('message_docs_server_url','http://messages.pronovix.net');
}

function message_docs_uninstall() {
  variable_del('message_docs_site_id');
  variable_del('message_docs_server_url');
}
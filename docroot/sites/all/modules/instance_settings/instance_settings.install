<?php

/**
 * @file
 * Instance settings install/uninstall procedures.
 */

/**
 * Implements hook_uninstall().
 */
function instance_settings_uninstall() {
  // Remove variables.
  variable_del('instance_settings_module_pools');
  variable_del('instance_settings_enable_modules');
  variable_del('instance_settings_disable_modules');
  variable_del('instance_settings_variables');
  variable_del('instance_settings_verbose_mode');
  variable_del('instance_settings_apply_on_page_load');
}

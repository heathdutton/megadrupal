<?php

/**
 * @file
 * Module's install/uninstall related hooks.
 */

/**
 * Implements hook_uninstall().
 */
function jquery_custom_scrollbar_uninstall() {
  variable_del('jquery_custom_scrollbar_pages');
  variable_del('jquery_custom_scrollbar_selectors');
  variable_del('jquery_custom_scrollbar_skin');
}

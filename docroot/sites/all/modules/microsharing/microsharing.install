/**
 * Implements hook_uninstall().
 */
function microsharing_uninstall() {
  variable_del('microsharing_enabled');
  variable_del('microsharing_visibility_path');
  variable_del('microsharing_services_code');
  variable_del('microsharing_services_code_social_services');
  variable_del('microsharing_services_code_color_icon');
  variable_del('microsharing_services_code_bg_icon');
  variable_del('microsharing_services_code_background_text');
  variable_del('microsharing_services_code_color_text');
  variable_del('microsharing_services_code_sharing_quote');
  variable_del('microsharing_services_code_sharing_images');
  variable_del('microsharing_services_code_sharing_page');
  variable_del('microsharing_token_api');
  variable_del('microsharing_token_api_signature');
  variable_del('microsharing_token_api_timestamp');
}
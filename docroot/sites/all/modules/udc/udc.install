/**
 * Implements hook_uninstall().
 *
 * Uninstall plugin
 * @return void
 */
function udc_uninstall() {
  require_once(dirname(__FILE__) . '/include/UserDataConnectorConfig.class.inc');
  $cfg = new UserDataConnectorConfig();
  $cfg->deleteDbVariables();
}

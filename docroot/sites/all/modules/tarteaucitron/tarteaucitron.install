<?php
/**
 * @file
 * Tarteaucitron: Add the tarteaucitron.js service.
 */

/**
 * Implements hook_uninstall().
 */
function tarteaucitron_uninstall() {
  variable_del('tarteaucitronUUID');
  variable_del('tarteaucitronToken');

  for ($i = 1; $i < 15; $i++) {
    variable_del('tarteaucitron_id_service' . $i);
    variable_del('tarteaucitron_titre_service' . $i);
    variable_del('tarteaucitron_img_service' . $i);
  }
}

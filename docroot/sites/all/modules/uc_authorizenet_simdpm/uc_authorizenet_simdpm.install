<?php

/**
 * @file
 *   Install file for uc_authorizenet_simdpm module.
 */

/**
 * Implements hook_uninstall().
 */
function uc_authorizenet_simdpm_uninstall() {
  global $conf;

  // Remove module variables.
  foreach (array_keys($conf) as $key) {
    if (strpos($key, 'uc_authnet_simdpm_') === 0) {
      variable_del($key);
    }
  }
}

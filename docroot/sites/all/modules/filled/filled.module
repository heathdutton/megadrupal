<?php

/**
 * Implements hook_element_info_alter().
 */
function filled_element_info_alter(&$type) {
  $type['form']['#after_build'][] = 'filled_after_build';
}

function filled_after_build($element, &$form_state) {
  $element['#attached']['js'][] = drupal_get_path('module', 'filled') . '/filled.js';
  return $element;
}

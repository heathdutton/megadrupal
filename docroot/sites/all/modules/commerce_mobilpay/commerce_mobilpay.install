<?php
/**
 * @file
 * Install, update and uninstall functions for commerce_bobilpay.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_mobilpay_uninstall() {
  // Simple DB query to get the names of our variables.
  variable_del('commerce_mobilpay_key_public');
  variable_del('commerce_mobilpay_key_private');
  variable_del('commerce_mobilpay_confirmurl');
  variable_del('commerce_mobilpay_cancelurl');
}

<?php

/**
 * @file
 * Install, update, and uninstall hooks for the Eloqua REST API module.
 */


/**
 * Implements hook_uninstall().
 */
function eloqua_rest_api_uninstall() {
  $field_keys =  array(
    'site_name',
    'login_name',
    'login_password',
    'base_url',
  );
  
  foreach ($field_keys as $field) {
    variable_del('eloqua_rest_api_' . $field);
  }
}

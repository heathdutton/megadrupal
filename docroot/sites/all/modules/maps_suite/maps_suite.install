<?php

/**
 * @file
 * Installation file for MaPS Suite module.
 */

/**
 * Implements hook_schema().
 */
function maps_suite_schema() {
  return array('cache_maps_suite' => drupal_get_schema_unprocessed('system', 'cache'));
}

/**
 * Implements hook_uninstall().
 */
function maps_suite_uninstall() {
  variable_del('maps_suite_log_path');
  variable_del('maps_suite_log_oberver_watchdog_partial');
}

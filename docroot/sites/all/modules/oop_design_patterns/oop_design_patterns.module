<?php

/**
 * @file
 * OOP Examples module.
 */

/**
 * Implements hook_menu().
 */
function oop_design_patterns_menu() {

  $items['oop-design-patterns'] = array(
    'title' => 'OOP Design Patterns',
    'page callback' => 'oop_design_patterns_page',
    'access callback' => TRUE,
  );

  return $items;
}

/**
 * Page callback.
 */
function oop_design_patterns_page() {

  $message = t('A variety of OOP Design Patterns code.');

  return array('#markup' => '<p>' . $message . '</p>');
}

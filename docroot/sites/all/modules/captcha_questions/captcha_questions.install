<?php

/**
 * @file
 * Uninstall functions for the Captcha questions module.
 */

/**
 * Implements hook_uninstall().
 */
function captcha_questions_uninstall() {

  // Delete variables.
  variable_del('captcha_questions_form_ids');
  variable_del('captcha_questions_question');
  variable_del('captcha_questions_answer');
  variable_del('captcha_questions_description');
  variable_del('captcha_questions_watchdog');
}

<?php

/**
 * @file
 * Install file for "Cricket Score Widget".
 */

/**
 * Implements hook_uninstall().
 */
function cricket_score_widget_uninstall() {
  variable_del('cricket_score_widget_width');
  variable_del('cricket_score_widget_height');

  db_delete('block')->condition('module', 'cricket_score_widget')->condition('delta', 'cricketscore')->execute();
}


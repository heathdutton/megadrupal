<?php
/**
 * @file
 * Main install file for the internet_defense_league module.
 */

/**
 * Implements internet_defense_league_uninstall().
 */
function internet_defense_league_uninstall() {
  drupal_load('module', 'internet_defense_league');

  $vars = internet_defense_league_variables();
  foreach ($vars as $v => $var) {
    variable_del($v);
  }
}
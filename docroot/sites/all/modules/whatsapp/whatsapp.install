<?php

/**
 * @file
 * Install, update and uninstall functions for the WhatsApp module.
 */

/**
 * Implements hook_uninstall().
 */
function whatsapp_uninstall() {
  variable_del('whatsapp_sitewide_phone');
  variable_del('whatsapp_sitewide_identifier');
  variable_del('whatsapp_sitewide_username');
  variable_del('whatsapp_sitewide_status');
}

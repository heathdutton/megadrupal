<?php

/**
 * @file
 * Install, update, and uninstall functions for the uc_tax_wa module.
 */

/**
 * Implements hook_uninstall().
 *
 * Removes variables inserted into the database by this module.
 */
function uc_tax_wa_uninstall() {

  variable_del('uc_tax_wa_default_rate');
  variable_del('uc_tax_wa_default_loc');
  variable_del('uc_tax_wa_show_loc');
  variable_del('uc_tax_wa_wholesale_roles');
  variable_del('uc_tax_wa_product_types');
  variable_del('uc_tax_wa_line_items');
}

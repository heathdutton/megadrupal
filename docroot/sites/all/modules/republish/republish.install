<?php
/**
 * @file
 * Install, update, and uninstall functions for the Republish module.
 */

/**
 * Implements hook_uninstall().
 */
function republish_uninstall() {
  variable_del('republish_link_text');
  variable_del('republish_title');
  variable_del('republish_guidelines');
  variable_del('republish_instructions');
  variable_del('republish_body');
  variable_del('republish_body_readonly');
  variable_del('republish_branding');
  variable_del('republish_link_css_class');
}

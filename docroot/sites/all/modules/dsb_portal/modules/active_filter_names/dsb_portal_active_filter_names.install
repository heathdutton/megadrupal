<?php

/**
 * @file
 * Module install logic.
 */

/**
 * Implements hook_install().
 */
function dsb_portal_active_filter_names_install() {
  // Make sure our module is always called last.
  db_update('system')
    ->fields(array(
      'weight' => 200
    ))
    ->condition('name', 'dsb_portal_active_filter_names')
    ->execute();
}

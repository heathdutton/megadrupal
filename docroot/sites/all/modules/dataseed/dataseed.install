<?php
/**
 * @file
 * Install file for the dataseed module.
 */

/**
 * Implements hook_uninstall().
 *
 * Variable deletion.
 */
function dataseed_uninstall() {
  variable_del('dataseed_scheme');
  variable_del('dataseed_host');
  variable_del('dataseed_port');
  variable_del('dataseed_path');
  variable_del('dataseed_user');
  variable_del('dataseed_pass');
}

<?php

/**
 * @file
 * Installation Process.
 */

/**
 * Implements hook_enable().
 */
function admin_menu_navbar_enable() {
  drupal_set_message(t('The Admin Menu Navbar module was successfully enabled.'), 'status');
}

/**
 * Implements hook_disable().
 */
function admin_menu_navbar_disable() {
  drupal_set_message(t('The Admin Menu Navbar module was successfully disable.'), 'status');
}

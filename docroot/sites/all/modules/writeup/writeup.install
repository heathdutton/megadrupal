<?php
/**
 * @file
 * writeup module install and uninstall hooks
 */

/**
 * Implements hook_uninstall().
 */
function writeup_uninstall() {
  // Remove variables.
  variable_del('writeup_incdir');
  variable_del('writeup_ver');
  variable_del('writeup_loc');
  variable_del('writeup_logerrors');
  variable_del('writeup_showerrors');
}


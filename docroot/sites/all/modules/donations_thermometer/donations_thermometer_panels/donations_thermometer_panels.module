<?php

/**
 * @file
 * Allows creation of a progress bar panels panes
 */

/**
 * Implements hook_ctools_plugin_directory().
 */
function donations_thermometer_panels_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/' . $plugin_type;
  }
}

<?php

/**
 * @file
 * Uninstall functions for the DBLog pager module.
 */

/**
 * Implements hook_uninstall().
 */
function dblog_pager_uninstall() {
  variable_del('dblog_pager_show_fl');
  variable_del('dblog_pager_bad_id_override');
  variable_del('dblog_pager_bad_id_last');
  variable_del('dblog_pager_fix_breadcrumbs');
}

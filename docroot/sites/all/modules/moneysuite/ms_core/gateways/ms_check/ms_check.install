/**
 * @file
 * Install, update and uninstall functions for the ms_check module.
 *
 */

/**
 * @file
 * Installation file for MS Check

 /**
 * Implements hook_uninstall().
 */
function ms_check_uninstall() {
  db_delete('variable')
    ->condition('name', 'ms_check_%%', 'LIKE')
    ->execute();
}

<?php

/**
 * @file
 * Handles installation for Commerce NZPost module.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_nzpost_uninstall() {
  db_delete('variable')
    ->condition('name', db_like('commerce_nzpost') . '%', 'LIKE')
    ->execute();
  cache_clear_all('variables', 'cache_bootstrap');
}

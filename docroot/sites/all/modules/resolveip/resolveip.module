<?php

/*
 * Implements hook_menu_alter(). Alters the core function to display the log entry.
 */
function resolveip_menu_alter(&$items) {
  $items['admin/reports/event/%']['page callback'] = 'resolveip_dblog_event';
  $items['admin/reports/event/%']['file'] = 'resolveip.admin.inc';
  $items['admin/reports/event/%']['file path'] = drupal_get_path('module', 'resolveip');
}
<?php

/**
 * @file
 * Install, update, and uninstall functions 
 * for the Pushretail For Ubercart module.
 */

/**
 * Implements hook_uninstall
 * Remove module settings. 
 */
function uc_pushretail_uninstall() {
  variable_del('uc_pushretail_site_key');
  variable_del('uc_pushretail_list_limit');
}

/**
 * Implements hook_uninstall().
 */
function form_maker_uninstall() {
  variable_del('form_maker_node_types');
  variable_del('form_maker_node_types_primary');
  if (variable_get('form_maker_header_array_string', '') != '') {
    $headers = explode('@@&&@@', variable_get('form_maker_header_array_string', ''));
    foreach ($headers as $header) {
      variable_del('form_maker_is_checked_' . $header);
      if (($header != 'date') && ($header != 'ip') && ($header != 'id')) {
        variable_del($header . '_search');
      }
    }
  }
  variable_del('form_maker_header_array_string');
  variable_del('form_maker_is_checked_all');
  // Delete theme search variable.
  variable_del('form_maker_search_theme_title');
  // Delete submissions search variables.
  variable_del('data_search_from');
  variable_del('data_search_to');
  variable_del('ip_search');
  variable_del('search_on');
  // Delete form_maker node from NODE TABLE.
  $form_maker_nodes_col = db_query("SELECT nid FROM {node} WHERE type=:type", array(':type' => 'form_maker'))->fetchCol();
  foreach ($form_maker_nodes_col as $form_maker_node_col) {
    db_query('DELETE FROM {node_comment_statistics} WHERE nid=:nid', array(':nid' => $form_maker_node_col));
    // Delete reports.
    variable_del('form_maker_view_reports' . $form_maker_node_col);
  }
  // Delete custom text after submit.
  variable_del('form_maker_custom_text_after_submit');
  db_query('DELETE FROM {node} WHERE type=:type', array(':type' => 'form_maker'));
  drupal_uninstall_schema('form_maker');
}


        'size' => 'big',
        'not null' => TRUE,
      ),
      'group_id' => array(
        'type' => 'int',
        'length' => 11,
        'not null' => TRUE,
      ),
      'date' => array(
        'type' => 'varchar',
        'length' => 128,
        'not null' => TRUE,
      ),
      'ip' => array(
        'type' => 'varchar',
        'length' => 128,
        'not null' => TRUE,
      ),
    ),
    'primary key' => array('id'),
  );
  $schema['form_maker_themes'] = array(
    'description' => 'Themes for form.',

<?php
function sponsoring404_install() {
  variable_set('sponsoring404_begunstigden', array());
}

function sponsoring404_uninstall() {
  variable_del('sponsoring404_begunstigden');
}

function sponsoring404_enable() {
  variable_set('site_404', 'sponsoring404');
  
  
}


function sponsoring404_disable() {
  variable_set('site_404', '');
}

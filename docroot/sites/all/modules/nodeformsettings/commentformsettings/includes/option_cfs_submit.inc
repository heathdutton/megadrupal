<?php
/**
 * @file
 * Hide the Revision log field.
 */

function _option_cfs_submit(&$form, &$form_state, $settings, $node) {
  if (!empty($settings['cfs_submit'])) {
    $form['actions']['submit']['#value'] = $settings['cfs_submit'];
  }
  else {
    $form['actions']['submit']['#value'] = t("Submit");
  }

  return $form;
}

/**
 * @file
 * Install, update, and uninstall functions for the commerce dtc module.
 */


/**
 * Implements hook_uninstall().
 */
function commerce_dtc_uninstall() {
  // Delete variable settings.
  variable_del('commerce_dtc_short_names');
  variable_del('commerce_dtc_use_pdas_config');
}

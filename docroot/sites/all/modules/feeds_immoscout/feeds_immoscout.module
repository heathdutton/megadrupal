<?php

/**
 * Implements hook_ctools_plugin_api().
 */
function feeds_immoscout_ctools_plugin_api($owner, $api) {
  if ($owner == 'feeds' && $api == 'plugins') {
    return array('version' => 1);
  }
}

/**
 * Implements hook_feeds_plugins().
 */
function feeds_immoscout_feeds_plugins() {
  $info = array();
  $info['FeedsImmoscoutFetcher'] = array(
    'name' => 'Immoscout fetcher',
    'description' => 'Fetch from Immoscout API.',
    'help' => '',
    'handler' => array(
      'parent' => 'FeedsFetcher',
      'class' => 'FeedsImmoscoutFetcher',
      'file' => 'FeedsImmoscoutFetcher.inc',
      'path' => drupal_get_path('module', 'feeds_immoscout'),
    ),
  );
  $info['FeedsImmoscoutParser'] = array(
    'name' => 'Immoscout parser',
    'description' => 'Parse Immoscout feeds.',
    'help' => '',
    'handler' => array(
      'parent' => 'FeedsParser',
      'class' => 'FeedsImmoscoutParser',
      'file' => 'FeedsImmoscoutParser.inc',
      'path' => drupal_get_path('module', 'feeds_immoscout'),
    ),
  );
  return $info;
}

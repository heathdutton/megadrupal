<?php
/**
 * @file
 * Install and uninstall functions for the POEditor module.
 */

/**
 * Implements hook_uninstall().
 */
function poeditor_uninstall() {
  variable_del("poeditor_token");
  variable_del("poeditor_project");
  variable_del("poeditor_send_all_terms_cron");
  variable_del("poeditor_get_all_translations_cron");
  variable_del("poeditor_next_cron_date");
}

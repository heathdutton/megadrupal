<?php
/**
 * @file
 * Install and uninstall hooks for Source Ad module.
 */

/**
 * Implements hook_install().
 */
function source_ad_uninstall() {
  // Remove variables.
  variable_del('source_ad_content');
  variable_del('source_ad_header_value');
  variable_del('source_ad_header_name');
  variable_del('source_ad_javascript_console_message');
}

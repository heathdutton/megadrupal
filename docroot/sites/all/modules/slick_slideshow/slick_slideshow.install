<?php
/**
 * @file
 * Installation actions for FlexSlider
 */

/**
 * Implements hook_install().
 */
function slick_slideshow_install() {
  variable_set('slick_slideshow_link', 'https://github.com/kenwheeler/slick/archive/master.zip');
}

/**
 * Implements hook_uninstall().
 */
function slick_slideshow_uninstall() {
  variable_del('slick_slideshow_link');
}

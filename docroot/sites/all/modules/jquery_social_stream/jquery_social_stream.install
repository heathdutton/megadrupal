<?php
/**
 * @file
 * Install and uninstall functions for jQuery Social Stream module.
 */

/**
 * Implements hook_uninstall().
 */
function jquery_social_stream_uninstall() {
  variable_del('jquery_social_stream_twitter_api_key');
  variable_del('jquery_social_stream_twitter_api_secret');
  variable_del('jquery_social_stream_twitter_access_token');
  variable_del('jquery_social_stream_twitter_access_token_secret');
  variable_del('jquery_social_stream_google_key');
  variable_del('jquery_social_stream_instagram_access_token');
  variable_del('jquery_social_stream_instagram_redirect_url');
  variable_del('jquery_social_stream_instagram_client_id');
}

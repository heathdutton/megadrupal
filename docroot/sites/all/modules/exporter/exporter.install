<?php


/**
 * Implements hook_schema().
 */
function exporter_schema() {
  $schema = array();

  $schema['cache_exporter'] = drupal_get_schema_unprocessed('system', 'cache');
  $schema['cache_exporter']['description'] = 'Cache table for the Exporter module to store exported fragments.';

  return $schema;
}


<?php

/**
 * @file
 * Installation file for Fortissimmo module
 */

/**
 * Implements hook_uninstall().
 */
function fortissimmo_uninstall() {
  variable_del('fortissimmo_file_path');
  variable_del('fortissimmo_last_cron');
  variable_del('fortissimmo_default_account');
  $accounts = fortissimmo_get_accounts();
  if ($accounts) {
    foreach ($accounts as $accountid => $accountname) {
      variable_del('fortissimmo_views_detail_view_' . $accountid);
    }
  }
}
  "require": {
    "symfony/yaml": "2.6.*",
    "graze/guzzle-jsonrpc": "~2.1",
    "t0k4rt/phpqrcode": "dev-master",
    "bitwasp/bitcoin-lib": "1.0.*",
    "bluedroplet/coins": "dev-master"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "t0k4rt/phpqrcode",
        "version": "dev-master",
        "source": {
          "url": "https://github.com/t0k4rt/phpqrcode",
          "type": "git",
          "reference": "origin/master"
        }
      }
    }
  ]
}

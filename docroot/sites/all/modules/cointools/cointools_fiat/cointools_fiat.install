<?php

/**
 * @file
 * Install file for Fiat.
 */

use Drupal\cointools_fiat\Fiat;

/**
 * Implements hook_install().
 */
function cointools_fiat_install() {
  variable_set('cointools_daemon.default_currency', '');
}

/**
 * Implements hook_uninstall().
 */
function cointools_fiat_uninstall() {
  variable_del('cointools_daemon.default_currency');
}

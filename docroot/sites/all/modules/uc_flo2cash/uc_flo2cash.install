<?php

/**
 * @file
 * Install, update and uninstall functions for the flo2cash payment gateway module.
 */

/**
 * Clear menu cache.
 */
function uc_flo2cash_update_6100(&$sandbox) {
  $ret = array();
  menu_cache_clear_all();
  return $ret;
}

/**
 * Implements hook_uninstall().
 */
function uc_flo2cash_uninstall() {
  variable_del('uc_flo2cash_web2pay_url');
  variable_del('uc_flo2cash_web2pay_verify_url');
  variable_del('uc_flo2cash_account_id');
}

<?php

/**
 * @file
 * Clean up after uninstall
 */

/**
 * Implements hook_uninstall().
 */
function youtube_api_uninstall() {
  variable_del('youtube_api_client_id');
  variable_del('youtube_api_secret');
  variable_del('youtube_api_redirect_location');
  variable_del('youtube_api_access_token');
}

<?php

/**
 * @file
 * Install, update and uninstall functions for the Volacci Automatr (TM)
 * module.
 */

/**
 * Implements hook_uninstall().
 */
function automatr_uninstall() {
  variable_del('automatr_implementation_code');
  variable_del('automatr_implementation_pages_pages');
  variable_del('automatr_implementation_pages_type');
  variable_del('automatr_implementation_roles');
}

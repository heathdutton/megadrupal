/**
 * @file
 * Provides install, updated, and uninstall functions for confident_captcha.
 */
 
/**
 * Implementation of hook_uninstall().
 */
function confident_captcha_new_uninstall() {
  // Delete all the confident_captcha variables and then clear the variable cache
  db_query("delete from {variable} where name like '%s'", 'confident_captcha_%');
  cache_clear_all('variables', 'cache');
}

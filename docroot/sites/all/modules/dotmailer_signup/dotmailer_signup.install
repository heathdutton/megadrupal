<?php

/**
 * @file
 * Contains uninstall functions for dotMailer integration module.
 */

/**
 * Implements hook_uninstall().
 */
function dotmailer_signup_uninstall() {
  variable_del('dotmailer_signup_selected_addressbook');
  variable_del('dotmailer_signup_name_include');
  variable_del('dotmailer_signup_name_field');
  variable_del('dotmailer_signup_optin_type');
  variable_del('dotmailer_signup_audience_type');
}
<?php

/**
 * @file
 * Module uninstall logic.
 */

/**
 * Implements hook_uninstall().
 */
function javascript_drupal_extension_uninstall() {
  variable_del('javascript_drupal_extension_ajax_callback_url');
  variable_del('javascript_drupal_extension_function_whitelist');
  variable_del('javascript_drupal_extension_allow_get_variables');
  variable_del('javascript_drupal_extension_parse_output_as_json');
  variable_del('javascript_drupal_extension_parse_content_type');
  variable_del('javascript_drupal_extension_parse_whitelist');
  variable_del('javascript_drupal_extension_log_usage_to_watchdog');
}

<?php

/**
 * @file
 * Install, update and uninstall functions for adsense on search.
 */

/**
 * Implements hook_uninstall().
 */
function adsense_search_uninstall() {
  // Remove module variables
  $result = db_query("SELECT name FROM {variable} WHERE name LIKE 'adsense_search__%'");
  while ($var_name = db_result($result)) {
    variable_del($var_name);
  }
}


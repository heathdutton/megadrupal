<?php

/**
 * @file
 * Nimble CRM Contact module install/schema hooks.
 */

/**
 * Implements hook_uninstall().
 */
function nimble_crm_contact_uninstall() {
  // Unset Nimble CRM variables.
  variable_del('nimble_crm_contact_send');
  variable_del('nimble_crm_contact_tags');
  variable_del('nimble_crm_contact_custom_fields');
}

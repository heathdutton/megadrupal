<?php

/**
 * @file
 * Install, update, and uninstall functions for the Statistics Blocks module.
 */

/**
 * Implements hook_uninstall().
 */
function statistics_blocks_uninstall() {
  // Remove variables.
  foreach (array('recent', 'not-found', 'access-denied', 'hits', 'pages', 'referrers') as $delta) {
    variable_del('statistics_blocks_' . $delta);
  }
}

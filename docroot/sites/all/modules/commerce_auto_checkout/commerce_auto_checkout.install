<?php
/**
 * @file
 * Update and uninstall hooks for the commerce_auto_checkout module.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_auto_checkout_uninstall() {
  variable_del('commerce_auto_checkout_disable_cart_page');
  variable_del('commerce_auto_checkout_auto_create_order');
  variable_del('commerce_auto_checkout_auto_create_products');
}

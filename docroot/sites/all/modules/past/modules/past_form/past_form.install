<?php

/**
 * @file
 * Installation hooks.
 */

/**
 * Implements hook_enable().
 *
 * Make sure Past forms hook_form_alter() is the last form_alter called.
 */
function past_form_install() {
  db_update('system')
    ->fields(array('weight' => 200))
    ->condition('name', 'past_form', '=')
    ->execute();
}

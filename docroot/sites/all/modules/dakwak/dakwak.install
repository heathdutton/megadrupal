/**
 * Implementation of hook_install().
 */
function dakwak_install(){
  drupal_set_message(t("Dakwak was installed successfully.<br />Log in to <a href=@href>dakwak</a> for advanced options to take over your website localization!", array('@href' => url('http://dakwak.com'))));
}

/**
 * Implementation of hook_uninstall().
 */
function dakwak_uninstall(){
	variable_del('dakwak_settings');
}
<?php
/**
 * @file
 * Install, update and uninstall functions for the ptv_timetable_api module.
 */

/**
 * Implements hook_uninstall().
 */
function ptv_timetable_api_uninstall() {
  // Remove variables.
  variable_del('ptv_timetable_api_security_key');
  variable_del('ptv_timetable_api_developer_id');
}

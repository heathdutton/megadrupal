<?php

/**
 * @file
 * Simplemeta_content_types install file.
 */

/**
 * Removes all variables.
 */
function simplemeta_content_types_uninstall() {
  // We should delete variables and stuff and things.
  db_delete('variable')
    ->condition('name', db_like('simplemeta_content_types_') . '%', 'LIKE')
    ->execute();
  cache_clear_all('variables', 'cache_bootstrap');
}

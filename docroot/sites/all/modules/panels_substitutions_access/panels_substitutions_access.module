/**
 * Implements hook_ctools_plugin_directory().
 */
function panels_substitutions_access_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'access') {
    return "plugins/access";
  }
}

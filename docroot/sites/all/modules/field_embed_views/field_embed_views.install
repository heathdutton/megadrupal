<?php

/**
 * @file
 * Installation functions for field_embed_views module.
 */

/**
 * Implements hook_field_schema().
 */
function field_embed_views_field_schema($field) {
  return array(
    'columns' => array(
      'settings' => array(
        'type' => 'blob',
        'not null' => FALSE,
        'size' => 'normal',
      ),
    ),
  );
}

<?php
/*
 * hook_install()
 */
function webform_marketing_install(){
  variable_set('webform_marketing_param', array());
  variable_set('webform_marketing_cookie_life_time', 6);
}

/*
 * hook_uninstall()
 */
function webform_marketing_uninstall(){
  variable_del('webform_marketing_param');
  variable_del('webform_marketing_cookie_life_time');  
}
<?php
/**
 * @file
 * Install, update, and uninstall functions for the ManyMail Views module.
 */

/**
 * Implements hook_uninstall().
 */
function manymail_views_uninstall() {
  variable_del('manymail_views');
  variable_del('manymail_views_allowed_groups');
  variable_del('manymail_views_allowed_views');
  variable_del('manymail_views_allowed_views_override');
}

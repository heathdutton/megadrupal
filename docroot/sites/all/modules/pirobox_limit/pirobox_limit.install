<?php
/**
 * @file
 * Install, update and uninstall functions for the Pirobox Limit module.
 */

/**
 * Implements hook_uninstall().
 */
function pirobox_limit_uninstall() {
  global $conf;

  foreach (array_keys($conf) as $key) {
    if (strpos($key, 'pirobox_limit_') === 0) {
      variable_del($key);
    }
  }
}

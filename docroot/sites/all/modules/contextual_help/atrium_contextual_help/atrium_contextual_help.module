include_once 'atrium_contextual_help.features.inc';
include_once 'atrium_contextual_help.theme.inc';

/*
 * Implements hook_ctools_plugin_directory
 */
function atrium_contextual_help_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/content_types';
  }
}

<?php

/**
 * @file
 * Contains uninstall function for Similar Entries Popup.
 */

/**
 * Implements hook_uninstall().
 */
function similar_popup_uninstall() {
  $variables = array(
    'similar_popup_position',
    'similar_popup_sound_notification',
  );

  foreach ($variables as $variable) {
    variable_del($variable);
  }
}

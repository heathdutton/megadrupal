<?php

/**
 * @file
 * Commerce Order Locking install
 */

/**
 * Implements hook_install().
 */
function commerce_order_locking_install() {
  variable_set('commerce_order_locking_status', array('completed'));
}

/**
 * Implements hook_uninstall().
 */
function commerce_order_locking_uninstall() {
  variable_del('commerce_order_locking_status');
}

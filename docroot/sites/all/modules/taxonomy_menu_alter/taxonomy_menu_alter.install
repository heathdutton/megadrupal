<?php
/**
 * @file
 * Install, update, and uninstall functions for the taxonomy_menu_alter module.
 */

/**
 * Implements hook_uninstall().
 */
function taxonomy_menu_alter_uninstall() {
  $vocabularies = taxonomy_get_vocabularies();

  foreach ($vocabularies as $vocabulary) {
    variable_del('taxonomy_menu_alter_subcategory_status_' . $vocabulary->vid);
  }
}

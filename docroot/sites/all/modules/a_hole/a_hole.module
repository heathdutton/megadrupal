<?php

/**
 * @file
 * The a-hole module tests if your JavaScript follows good dependency injection
 * and code isolation practices.
 */

/**
 * Implements hook_init().
 */
function a_hole_init() {
  drupal_add_js(drupal_get_path('module', 'a_hole') . '/boom.js', array(
    'every_page' => TRUE,
    'group' => JS_LIBRARY,
  ));
}
<?php

/**
 * @file
 * Tests for digitalnzapi.module.
 */

/**
 * Helper class for Digital NZ API test.
 * Requires API key to be defined.
 */
class DigitalNZAPITestCase extends DrupalWebTestCase {
  protected $admin_user;

  function setUp() {
    parent::setUp('digitalnzapi');

    $this->apikey = '';

    $this->admin_user = $this->drupalCreateUser(array('access administration pages', 'administer modules'));
    $this->drupalLogin($this->admin_user);
  }

}
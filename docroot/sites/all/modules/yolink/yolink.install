<?php
/**
 * Implementation of hook_uninstall().
 */
function yolink_uninstall() {
  variable_del('yolink_api_key');
  variable_del('yolink_logo');
  variable_del('yolink_max_results');
  variable_del('yolink_show_hide');
  variable_del('yolink_show_share');
  variable_del('yolink_show_google_docs');
  variable_del('yolink_tos_agree');
}
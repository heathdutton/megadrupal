<?php
/**
* @file
* Install, update, and uninstall functions for module.
*/

/**
 * Implements hook_uninstall().
 */
function site_under_construction_uninstall() {
  variable_del('site_under_construction_enable');
  variable_del('site_under_construction_title');
  variable_del('site_under_construction_favicon');
  variable_del('site_under_construction_templates_path');
  variable_del('site_under_construction_templates');
  
}
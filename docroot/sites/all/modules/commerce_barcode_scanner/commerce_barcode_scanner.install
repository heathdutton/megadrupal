<?php

/**
 * @file
 * Handle install and uninstall tasks.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_barcode_scanner_uninstall() {
  // Clean up our variables.
  module_load_include('module', 'commerce_barcode_scanner');
  foreach (commerce_barcode_scanner_default_variables() as $var => $default) {
    variable_del($var);
  }
}

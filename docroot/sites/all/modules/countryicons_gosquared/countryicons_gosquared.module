<?php

/**
 * @file
 * GoSquared flag iconsets for Country Icons.
 */

/**
 * Implements hook_iconset_info().
 */
function countryicons_gosquared_iconset_info() {
  $iconsets = array();

  $iconset = new stdClass();
  $iconset->key = 'gosquared_flat_small';
  $iconset->name = t('GoSquared (flat, small)');
  $iconset->description = t('GoSquared Flag Icon Set (flat, 14x11)');
  $iconset->width = '14';
  $iconset->height = '11';
  $iconset->extension = 'png';
  $iconset->path = drupal_get_path('module', 'countryicons_gosquared') . '/flat-small';
  $iconsets[$iconset->key] = $iconset;

  $iconset = new stdClass();
  $iconset->key = 'gosquared_flat_large';
  $iconset->name = t('GoSquared (flat, large)');
  $iconset->description = t('GoSquared Flag Icon Set (flat, 60x40)');
  $iconset->width = '60';
  $iconset->height = '40';
  $iconset->extension = 'png';
  $iconset->path = drupal_get_path('module', 'countryicons_gosquared') . '/flat-large';
  $iconsets[$iconset->key] = $iconset;

  $iconset = new stdClass();
  $iconset->key = 'gosquared_shiny_small';
  $iconset->name = t('GoSquared (shiny, small)');
  $iconset->description = t('GoSquared Flag Icon Set (shiny, 16x13)');
  $iconset->width = '16';
  $iconset->height = '13';
  $iconset->extension = 'png';
  $iconset->path = drupal_get_path('module', 'countryicons_gosquared') . '/shiny-small';
  $iconsets[$iconset->key] = $iconset;

  $iconset = new stdClass();
  $iconset->key = 'gosquared_shiny_large';
  $iconset->name = t('GoSquared (shiny, large)');
  $iconset->description = t('GoSquared Flag Icon Set (shiny, 62x42)');
  $iconset->width = '62';
  $iconset->height = '42';
  $iconset->extension = 'png';
  $iconset->path = drupal_get_path('module', 'countryicons_gosquared') . '/shiny-large';
  $iconsets[$iconset->key] = $iconset;

  return $iconsets;
}

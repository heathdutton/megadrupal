<?php

/**
 * @file
 * Install, update and uninstall functions
 */

/**
 * Implements hook_uninstall().
 */
function commerce_order_ftp_export_uninstall() {
  variable_del('order_ftp_export_ftp_host');
  variable_del('order_ftp_export_ftp_username');
  variable_del('order_ftp_export_ftp_password');
  variable_del('order_ftp_export_ftp_port');
  variable_del('order_ftp_export_ftp_directory');
}
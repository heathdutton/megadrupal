<?php
/**
 * @file
 * The install file of the Weather.com.au module.
 */

/**
 * Implements hook_uninstall().
 */
function weathercomau_uninstall() {
  variable_del('weathercomau_blocks');
  variable_del('weathercomau_cache_lifetime');
  variable_del('weathercomau_num_widgets');
  variable_del('weathercomau_pressure_unit');
  variable_del('weathercomau_rainfall_unit');
  variable_del('weathercomau_temperature_unit');
  variable_del('weathercomau_windspeed_unit');
}

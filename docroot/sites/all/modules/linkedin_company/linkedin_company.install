<?php
/**
 * @file
 * Uninstall function for the linkedin company status module.
 */

/**
 * Implements hook_uninstall().
 */
function linkedin_company_uninstall() {
  variable_del('linkedin_company_company_id');
  variable_del('linkedin_company_oauth_token');
  variable_del('linkedin_company_oauth_activated');
  variable_del('linkedin_company_oauth_expires');
  variable_del('linkedin_company_reminder_days');
}

<?php

/**
 * @file
 * Install tasks for logio_harvester
 */

/**
 * Implements hook_uninstall().
 */
function logio_harvester_uninstall() {
  variable_del('logio_harvester_nodename');
  variable_del('logio_harvester_server');
  variable_del('logio_harvester_port');
  variable_del('logio_harvester_debug');
  variable_del('logio_harvester_format');
  variable_del('logio_harvester_enable');
}


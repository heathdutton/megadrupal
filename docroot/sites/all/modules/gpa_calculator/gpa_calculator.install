<?php

/**
 * @file
 * Provides uninstall on variables.
 */

/**
 * Implements hook_uninstall().
 */
function gpa_calculator_uninstall() {
  // Delete variables.
  variable_del('gpa_calculator_school_name');
  variable_del('gpa_calculator_instructions');
  variable_del('gpa_calculator_grades');
  variable_del('gpa_calculator_prev_gpa_instructions');
  variable_del('gpa_calculator_prev_credits_instructions');
}

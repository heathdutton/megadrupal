#!/bin/sh
# to run Solr in different port use -Djetty.port parameter
# other JVM useful parameters:
# -XX:+UseParallelGC
# -XX:+AggressiveOpts
# -XX:+DisableExplicitGC
# -Djava.util.logging.config.file=logging.properties

java -DSTOP.PORT=8079 -DSTOP.KEY=ftasolrstop -server -Xms512M -Xmx1024M -Djava.util.logging.config.file=logging.properties -jar start.jar &

<?php

/**
 * @file
 * Install, update, and uninstall functions for the pager for content type
 * module.
 */

/**
 * Implements hook_uninstall().
 */
function pager_for_content_type_uninstall() {
  variable_del('pager_for_content_type_previous_text');
  variable_del('pager_for_content_type_next_text');
  variable_del('pager_for_content_type_node_types');
}

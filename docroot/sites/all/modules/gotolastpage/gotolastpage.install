<?php
/**
 * @file
 * Install, update and uninstall functions for the gotolastpage module.
 */

/**
 * Implements hook_uninstall().
 */
function gotolastpage_uninstall() {
  // Delete module variables.
  db_delete('variable')
    ->condition('name', 'gotolastpage_%', 'LIKE')
    ->execute();
  cache_clear_all('variables', 'cache_bootstrap');
}

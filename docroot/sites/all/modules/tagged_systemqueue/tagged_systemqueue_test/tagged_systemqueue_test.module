<?php

function tagged_systemqueue_test_query_queue_claimItem_alter(QueryAlterableInterface $query) {
  if (drupal_is_cli()) {
    drush_log('hook_query_TAG_alter called for queue: ' . $query->getMetaData('name'), 'ok');
  }
}

function tagged_systemqueue_test_cron_queue_info() {
  return array(
    'tagged_systemqueue_test' => array(
      'worker callback' => 'tagged_systemqueue_test_queue_worker',
    ),
  );
}

function tagged_systemqueue_test_queue_worker($data) {
  if (drupal_is_cli()) {
    drush_log('Processing item: ' . print_r($data,TRUE), 'ok');
  }
}

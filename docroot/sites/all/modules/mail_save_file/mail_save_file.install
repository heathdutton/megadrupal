<?php
/**
 * @file
 * Installation hooks for Mail save file.
 */

/**
 * Implements hook_uninstall().
 */
function mail_save_file_uninstall() {
  variable_del('mail_save_file_enabled');
  variable_del('mail_save_file_name');
  variable_del('mail_save_file_directory');
  variable_del('mail_save_file_whitelist');
  variable_del('mail_save_file_blacklist');
  variable_del('mail_save_file_message_id_to_name_mapping');
}

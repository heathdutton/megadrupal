<?php

/**
 * @file
 * Installation file for Commerce Everyday module
 */


/**
 * Implements hook_uninstall().
 */
function commerce_everyday_uninstall() {
  db_delete('variable')
  ->condition('name', 'commerce_everyday_%', 'LIKE')
  ->execute();
  cache_clear_all('variables', 'cache_bootstrap');
}

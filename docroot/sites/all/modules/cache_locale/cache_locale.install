<?php
/**
 * @file
 * Installation file for Cache Heuristic.
 */

/**
 * Implements hook_schema().
 */
function cache_locale_schema() {
  $schema = array();
  $cache_schema = drupal_get_schema_unprocessed('system', 'cache');
  $description = 'Cache table for locales';
  $schema['cache_locale'] = $cache_schema;
  $schema['cache_locale']['description'] = $description;
  return $schema;
}

<?php

/**
 * @file
 * harmony_print.install
 */

/**
 * Implements hook_enable().
 */
function harmony_print_enable() {
  // Clear the menu cache so our /print paths appear.
  cache_clear_all(NULL, 'cache_menu');
}

/**
 * Implements hook_uninstall().
 */
function harmony_print_uninstall() {
  variable_del('harmony_print_enable_posts');
}

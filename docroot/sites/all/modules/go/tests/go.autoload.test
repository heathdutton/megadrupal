<?php

class GoAutoloadTestCase extends GoUnitTestCase {

  public function getInfo() {
    return array(
      'name' => 'Go Autoloader',
      'description' => "Make sure the autoloader feature is working correctly.",
      'group' => 'Go Module'
    );
  }

  public function testAutoloader() {
    $this->assertTrue(class_exists('Drupal\go\Tests\Foo'));
  }

  public function testCoreKeyValue() {
    $this->assertTrue(class_exists('Drupal\Core\KeyValueStore\KeyValueDatabaseFactory'));
  }

}

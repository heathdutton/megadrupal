<?php

/**
 * @file
 * Uninstall function for the commerce_rbkmoney module.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_rbkmoney_uninstall() {
  variable_del('commerce_rbkmoney_eshopId');
  variable_del('commerce_rbkmoney_recipientCurrency');
  variable_del('commerce_rbkmoney_secretKey');
  variable_del('commerce_rbkmoney_preference');
  variable_del('commerce_rbkmoney_log');
  variable_del('commerce_rbkmoney_ip');
}

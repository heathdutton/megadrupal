<?php

/**
 * @file
 * Install, update and uninstall functions for the Media: TuDou module.
 */

/**
 * Implement hook_install().
 */
function media_tudou_install() {
  return array();
}

/**
 * Implement hook_uninstall().
 */
function media_tudou_uninstall() {
  foreach (media_tudou_variable_default() as $variable => $value) {
    media_tudou_variable_del($variable);
  }
  return array(array('success' => TRUE, 'query' => "Deleted all variables in the Media: TuDou namespace."));
}


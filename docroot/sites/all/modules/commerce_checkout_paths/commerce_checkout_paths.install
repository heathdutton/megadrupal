<?php
/**
 * @file
 * Uninstall file with required hooks implementations.
 */

/**
 * Implements hook_uninstall().
 *
 * Removing all variables, which has been set via this module.
 */
function commerce_checkout_paths_uninstall() {
  db_delete('variable')
    ->condition('name', 'commerce_checkout_paths%', 'LIKE')
    ->execute();
}

/**
 * Implements hook_update_N().
 */
function commerce_checkout_paths_update_7001() {
  variable_del('commerce_checkout_paths_complete');
}

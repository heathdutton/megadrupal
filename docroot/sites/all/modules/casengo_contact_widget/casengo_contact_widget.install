<?php

/**
 * @file
 * File to uninstall custom variables.
 */

/**
 * Implements hook_uninstall().
 */
function casengo_contact_widget_uninstall() {
  // Get global variable array.
  global $conf;
  foreach (array_keys($conf) as $key) {
    // Find the variables that have the module prefix.
    if (strpos($key, 'casengo_contact_widget_') === 0) {
      // Delete the all the casengo contact widget variables.
      variable_del($key);
    }
  }
}

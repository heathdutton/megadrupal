<?php

class XtoolsCallablesWebTestCase extends XtoolsWebTestCase {

  static function getInfo() {
    return array(
      'name' => "Callables",
      'group' => 'Xtools',
      'dependencies' => array('xtools'),
    );
  }

  function setUp(array $modules = array()) {
    parent::setUp($modules + array('xtools'));
  }

  function testCoreHookReturnValue() {
    $this->XtoolsAssertCallables(array('xtools'), array('drupal', 'xtools'));
  }
}
<?php

/**
 * @file
 * Installation file for Email Domain Restriction module.
 */

/**
 * Implements hook_uninstall().
 */
function email_domain_restriction_uninstall() {
  variable_del('email_domain_restriction_list');
  variable_del('email_domain_restriction_message');
  variable_del('email_domain_restriction_behaviour');
  variable_del('email_domain_restriction_apply_current_fields');
  variable_del('email_domain_restriction_message_to_change');
}

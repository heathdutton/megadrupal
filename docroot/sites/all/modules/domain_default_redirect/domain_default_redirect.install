<?php
/**
 * @file
 *
 * Register hook_domain_bootstrap_lookup in the domain_bootstrap module list
 * so it will be evaluated on bootstrapping.
 * Unregistration is handled automatically upon disabling the module.
 */
 
/**
 * Implements hook_enable().
 */
function domain_default_redirect_enable() {
  domain_bootstrap_register();
}

/**
 * Implements hook_uninstall().
 */
function domain_default_redirect_uninstall() {
  variable_del('domain_default_redirect_url');
}
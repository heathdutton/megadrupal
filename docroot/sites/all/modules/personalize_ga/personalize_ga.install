<?php

/**
 * @file
 * Personalize Google Analytics - Installation file.
 */

/**
 * Implements hook_uninstall().
 */
function personalize_ga_uninstall() {
  variable_del('personalize_ga_tracking_id');
  variable_del('personalize_ga_send_personalize_decisions');
  variable_del('personalize_ga_tracked_personalize_decisions');
  variable_del('personalize_ga_send_visitor_actions');
  variable_del('personalize_ga_tracked_visitor_actions');
}

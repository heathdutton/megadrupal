<?php

/**
 * @file
 * Install/uninstall hooks for Acobot module.
 */

/**
 * Implements hook_uninstall().
 */
function acobot_uninstall() {
  variable_del('acobot_token');
  variable_del('acobot_visibility_roles');
  variable_del('acobot_roles');
  variable_del('acobot_pages');
  variable_del('acobot_visibility');
}

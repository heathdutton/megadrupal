function iati_import_data_orgs_migrate_api() {
  $api = array(
    'api' => 2,
    'migrations' => array(
      'DataParticipateOrg' => array('class_name' => 'DataParticipateOrg'),
      'DataReportOrg' => array('class_name' => 'DataReportOrg'),		
      'RelationOrgRoleFunding' => array('class_name' => 'RelationOrgRoleFunding'),
      'RelationOrgRoleExtending' => array('class_name' => 'RelationOrgRoleExtending'),
      'RelationOrgRoleImplementing' => array('class_name' => 'RelationOrgRoleImplementing'),
      'RelationOrgRoleAccountable' => array('class_name' => 'RelationOrgRoleAccountable'),
      'DataParticipateOrg' => array('class_name' => 'DataParticipateOrg'),
    ),
  );
  return $api;
}

function iati_import_data_orgs_get_org_id($code) {
 // node_load()
   $result = db_select('node','mt')
  ->fields('mt', array('nid'))
  ->condition('field_iati_identifier', $code)
  ->execute()
  ->fetchAssoc();
   
}


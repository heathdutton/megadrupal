<?php
/**
 * @file
 * Nepali calendar installation.
 */

/**
 * Implements hook_uninstall().
 */
function nepali_calendar_uninstall() {
  // Delete variables.
  variable_del('nepali_calendar_nepali_date_format');
  variable_del('nepali_calendar_show_nepal_time');
  variable_del('nepali_calendar_nepal_time_format');
  variable_del('nepali_calendar_show_date_label');
}

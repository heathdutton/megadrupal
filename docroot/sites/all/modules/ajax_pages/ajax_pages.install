<?php

/**
 * @file
 * Install, update, and uninstall functions for the Ajax pages module.
 */

/**
 * Implements hook_uninstall().
 */
function ajax_pages_uninstall() {
  // Delete variables.
  variable_del('ajax_pages_wrapper_selector');
  variable_del('ajax_pages_links_selector');
  variable_del('ajax_pages_suppress_paths');
}

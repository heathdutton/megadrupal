<?php

/**
 * @file
 * OOP Examples module.
 */

/**
 * Implements hook_menu().
 */
function oop_examples_menu() {

  $items['oop-examples'] = array(
    'title' => 'OOP Examples',
    'page callback' => 'oop_examples_page',
    'access callback' => TRUE,
  );

  return $items;
}

/**
 * Page callback.
 */
function oop_examples_page() {

  $message = t('A variety of OOP example code.');

  return array('#markup' => '<p>' . $message . '</p>');
}

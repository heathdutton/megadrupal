<?php

/**
 * @file
 * Install, update and uninstall functions for the amocrm_widget module.
 */

/**
 * Implements hook_install().
 */
function amocrm_widget_install() {
  variable_set('amocrm_widget_user_api', array());
}

/**
 * Implements hook_uninstall().
 */
function amocrm_widget_uninstall() {
  variable_del('amocrm_widget_user_api');
}

<?php

/**
 * @file
 * Install, update and uninstall functions for the amocrm_widget_pages module.
 */

/**
 * Implements hook_install().
 */
function amocrm_widget_mail_install() {
  variable_set('amocrm_widget_pages_link_paths', array());
}

/**
 * Implements hook_uninstall().
 */
function amocrm_widget_mail_uninstall() {
  variable_del('amocrm_widget_pages_link_paths');
}

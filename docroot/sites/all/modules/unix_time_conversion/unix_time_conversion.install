<?php

/**
 * @file
 * Uninstallation of variables from variables table.
 */

/**
 * Implements hook_uninstall().
 */
function unix_time_conversion_uninstall() {
  /*
   * Deleting variables on module uninstall.
   */
  variable_del('unix_time_conversion_timestamp_field_title');
  variable_del('unix_time_conversion_timestamp_field_description');
  variable_del('unix_time_conversion_time_to_date_output_format');
  variable_del('unix_time_conversion_date_field_title');
  variable_del('unix_time_conversion_time_field_title');
}

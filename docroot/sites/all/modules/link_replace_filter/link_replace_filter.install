<?php

/**
 * @file
 * Install, update, and uninstall functions for the Link Replace Filter module.
 */

/**
 * Implements hook_uninstall().
 */
function link_replace_filter_uninstall() {
  $link_replace_filter_delete_rows = db_delete('filter')
    ->condition('module', 'link_replace_filter')
    ->execute();
}

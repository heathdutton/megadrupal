<?php

/**
 * @file
 * Deletes variable when uninstalling module.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_attributes_date_uninstall() {
  variable_del('commerce_attributes_date_date_format');
  variable_del('commerce_attributes_date_allowed_attributes');
  variable_del('commerce_attributes_date_product_reference_field_name');
}

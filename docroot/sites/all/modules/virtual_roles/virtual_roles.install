<?php
/**
 * @file
 * Install, update, and uninstall functions for the Virtual Roles module.
 */

/**
 * Implements hook_install().
 */
function virutal_roles_install() {
  db_update('system')
    ->fields(array('weight' => -1))
    ->condition('name', 'virtual_roles', '=')
    ->execute();
}

/**
 * Implements hook_uninstall().
 */
function virtual_roles_uninstall() {
}


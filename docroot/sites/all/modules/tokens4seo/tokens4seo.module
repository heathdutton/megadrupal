/**
 * Implements hook_menu()
 */
function tokens4seo_menu() {

	$items = array();

	$items['admin/config/search/tokens4seo'] = array(
		'title'	=> 'Tokens for seo',
		'description' => 'Create value for some advanced seo options',
		'page callback' => 'drupal_get_form',
		'page arguments' => array('variable_group_form', 'token4seo_variables'),
		'access arguments' => array('administer site configuration'),
	);

	return $items;
}

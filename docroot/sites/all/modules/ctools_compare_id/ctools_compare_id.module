<?php

/**
 * @file
 * Primarily Drupal hooks to manipulate Ctools Compare ID.
 *
 * This is the main module file for Ctools Compare ID.
 */

/**
 * Implements hook_ctools_plugin_directory().
 */
function ctools_compare_id_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools' && $plugin == 'access') {
    return 'plugins/access';
  }
}

<?php
/**
 * @file migrate_parcel.install
 * Contains install hooks.
 */

/**
 * Implements hook_disable().
 */
function migrate_parcel_disable() {
  $parcels = migrate_parcel_get_parcels();
  foreach ($parcels as $machine_name => $migration_arguments) {
    Migration::deregisterMigration($machine_name);
  }  
}

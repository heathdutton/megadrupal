<?php
/**
 * @file
 * Installation file for the Lotame module.
 */

/**
 * Implements hook_uninstall().
 */
function notame_uninstall() {
  drupal_variable_del('lotame_client_id');
  drupal_variable_del('lotame_url');
  drupal_variable_del('lotame_visibility');
  drupal_variable_del('lotame_pages');
}

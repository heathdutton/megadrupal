<?php
/**
 * @file
 * Install file for theming Unity3D player content in a web player.
 */

/**
 * Implements hook_uninstall().
 */
function unity3d_field_uninstall() {
  // Delete all variables that were set by the module.
  variable_del('unity3d_field_default_image');
  variable_del('unity3d_field_alt_text');
  variable_del('unity3d_field_dimensions');
}

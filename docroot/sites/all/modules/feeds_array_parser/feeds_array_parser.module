<?php

/**
 * Implements hook_feeds_plugins().
 */
function feeds_array_parser_feeds_plugins() {
  $path = drupal_get_path('module', 'feeds_array_parser') .'/plugins';
  $info['FeedsArrayParser'] = array(
    'name' => 'PHP Array parser',
    'description' => 'Parse data in a serialized array format.',
    'handler' => array(
      'parent' => 'FeedsParser',
      'class' => 'FeedsArrayParser',
      'file' => 'FeedsArrayParser.inc',
      'path' => $path,
    ),
  );
  
  return $info;
}
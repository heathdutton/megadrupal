<?php
/**
 * @file
 * Main module file for Wildfire signup module
 */

/**
 * Implements hook_install().
 */
function wildfire_signup_install() {
  // Sets up the default
  module_load_include('module', 'wildfire_signup', 'wildfire_signup');
  wildfire_signup_get_list();
}

/**
 * Implements hook_uninstall().
 */
function wildfire_signup_uninstall() {
  // Remove leftover system variables
  variable_del('wildfire_signup_list');
}

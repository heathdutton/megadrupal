<?php
/**
 * @file
 * Install, update and uninstall functions for the twitfaves module.
 *
 */

function twitfaves_uninstall() {
  variable_del('twitfaves_count');
  variable_del('twitfaves_screen_name');
  variable_del('twitfaves_interval');
  variable_del('twitfaves_link_urls');
  variable_del('twitfaves_filter');
}

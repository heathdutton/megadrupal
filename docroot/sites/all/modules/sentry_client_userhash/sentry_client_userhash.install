/**
 * @file
 * Installation tasks for the sentry userhash client.
 */

/**
 * Implements hook_install().
 */
function sentry_client_userhash_install() {
  variable_set('sentry_client_userhash_settings', 0); // Admin user monitoring.
}

/**
 * Implements hook_uninstall().
 */
function sentry_client_userhash_uninstall() {
  variable_del('sentry_client_userhash_settings');
}

<?php

/**
 * Implements hook_browser_details_plugin().
 */
function bd_javascript_browser_details_plugin() {
    return array(
        'javascript' => array(
            'name' => 'javascript',
            'title' => 'Javascript',
            'icon' => drupal_get_path('module', 'bd_javascript') . '/images/bd_javascript.png',
            'js' => drupal_get_path('module', 'bd_javascript') . '/js/bd_javascript.js',
            'content' => 'bd_javascript_load',
        ),
    );
}

function bd_javascript_load() {
    return 'Disabled';
}

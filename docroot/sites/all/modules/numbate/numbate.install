<?php
/**
 * @file
 * Installation file for the Numbate module.
 */

/**
 * Implements hook_uninstall().
 */
function numbate_uninstall() {
  drupal_variable_del('numbate_client_id');
  drupal_variable_del('numbate_bf_html');
  drupal_variable_del('numbate_tp_url');
  drupal_variable_del('numbate_js_url');
}

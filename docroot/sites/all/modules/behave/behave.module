<?php

/**
 * @file
 * Drupal.behave JS helper
 */

/**
 * Implements hook_preprocess_HOOK().
 */
function behave_preprocess_page(&$variables) {
  drupal_add_js(
    drupal_get_path('module', 'behave') . '/js/behave.min.js', array(
    'type' => 'file',
    'group' => JS_LIBRARY,
    'weight' => 0,
    'every_page' => TRUE,
  ));
}

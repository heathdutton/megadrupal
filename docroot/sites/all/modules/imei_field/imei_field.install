<?php

/**
 * @file
 * Defines schema for ISBN fields
 */

/**
 * Implements hook_field_schema().
 */
function imei_field_field_schema($field) {
  return array(
    'columns' => array(
      'imei' => array(
        'type' => 'varchar',
        'length' => 15,
        'not null' => FALSE,
      ),
    ),
  );
}

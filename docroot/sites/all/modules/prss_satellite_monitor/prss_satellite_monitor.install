<?php

/**
 * @file
 * Contains install/uninstall hooks.
 */

/**
 * Implements hook_uninstall().
 */
function prss_satellite_monitor_uninstall() {
  $variables = array(
    'prss_satellite_monitor_number',
    'prss_satellite_monitor_autorefresh',
    'prss_satellite_monitor_refresh_interval',
    'prss_satellite_monitor_json',
  );
  foreach ($variables as $variable) {
    variable_del($variable);
  }
}

/**
 * @file
 * Install, uninstall and update scripts for Commerce Checkout Products List.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_checkout_products_list_uninstall() {
  variable_del('commerce_checkout_products_list_order_only');
  variable_del('commerce_checkout_products_list_price');
  variable_del('commerce_checkout_products_list_view');
}

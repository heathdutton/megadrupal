<?php

/**
 * @file
 * Provides a UI for administering BACnet.
 */

/**
 * Implements hook_menu().
 */
function bacnet_menu() {
  $items['admin/config/services/bacnet'] = array(
    'access arguments' => array('administer bacnet'),
    'title' => 'BACnet Block',
    'description' => 'Manage settings for the "BACnet Block".',
    'page callback' => 'drupal_get_form',
    'page arguments' => array('bacnet_ui_admin_settings'),
    'file' => 'includes/bacnet_ui.admin.inc',
  );

  return $items;
}

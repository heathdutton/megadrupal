<?php

/**
 * @file
 * Install and uninstall functions for the Feedmine module.
 */

/**
 * Implementation of hook_uninstall().
 */
function feedmine_uninstall() {
  // Remove variables set by Feedmine module.
  $feedmine_vars = array(
    'feedmine_rmurl',
    'feedmine_rmapikey',
    'feedmine_rmuid',
    'feedmine_rmprojectid',
  );
  array_walk($feedmine_vars, 'variable_del');
}

name = Da Vinci Theme
description = Da Vinci Theme - Drupal 7 Full theme with the <b>best Sass's components</b>. <br />Developed by <b>Front-end Team</b> of <a href="http://ladrupalera.com">@ladrupalera</a> by <a href="http://emergya.com">Emergya</a> & <b>collaborators:</b> <a href="https://www.drupal.org/u/enno">@Enno</a>, <a href="https://www.drupal.org/u/capynet">@Capynet</a>, <a href="https://www.drupal.org/u/nicobot">@Nicobot</a>.<br /> <b>Important!!</b> Require Jquery Update & Libraries module.
core = 7.x
engine = phptemplate

stylesheets[all][] = css/lib.css
stylesheets[all][] = css/main.css

scripts[] = js/da-vinci.js
scripts[] = js/custom.js

; Panels layouts.
plugins[panels][layouts] = plugins/layouts
plugins[panels][styles] = plugins/styles

regions[top_bar]        = Top Bar
regions[header]         = Header
regions[preface]        = Preface
regions[content_top]    = Content Top
regions[sidebar_first]  = First Sidebar
regions[sidebar_second] = Second Sidebar
regions[help]           = Help
regions[content]        = Content
regions[footer]         = Footer

features[] = logo
features[] = name
features[] = slogan
features[] = node_user_picture
features[] = comment_user_picture
features[] = comment_user_verification
features[] = favicon
features[] = main_menu
features[] = secondary_menu
settings[breadcrumbs]  = 1

settings[styleguide] = 1
settings[debug] = 1

; Exclude Css's.
;settings[css_exclude][]  = ''

; Exclude JS's.
settings[js_exclude][]  = 'misc/tableheader.js'

; BreakPoint Module configurations 
;breakpoints[mobile] = (min-width: 0px)
;breakpoints[narrow] = (min-width: 560px)
;breakpoints[wide] = (min-width: 704px)
;breakpoints[tv] = only screen and (min-width: 3456px)
;multipliers[mobile][] = 1.5x
;multipliers[mobile][] = 2x

; Information added by Drupal.org packaging script on 2015-08-25
version = "7.x-2.4"
core = "7.x"
project = "da_vinci"

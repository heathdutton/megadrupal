
; name, description, screenshot, version - these are necessary for theme list
name        = "OM Eco"
description = "This a subtheme of OM base theme. For more info, read the <a href="http://drupal.org/node/1056254">OM Theme Documentation</a>"
screenshot  = "screenshot.png"

base theme = om

; compatibilities
core        = "7.x"
engine      = phptemplate

; default stylesheets

stylesheets[all][] = "css/style.css"

; jquery scripts
scripts[]       = "js/script.js"

; default regions, on the left inside the brackets 
; are the variable names used in "page.tpl.php",
; on right side are the names necessary for blocks list

; IMPORTANT: If you are adding your custom regions, make sure you copy 
; /sites/all/themes/om/core/tpl/region.tpl.php to your subtheme folder
; the same rule applies to custom node.tpl.php and block.tpl.php

regions[header_block] = Header Block
regions[highlighted] = Highlighted
regions[highlighted_left] = Highlighted column left
regions[highlighted_middle] = Highlighted column middle
regions[highlighted_right] = Highlighted column right
regions[content]        = Content
regions[sidebar_second] = "Sidebar - Right"
regions[footer_bottom_left] = Footer bottom column left
regions[footer_bottom_right] = Footer bottom column right
regions[footer_bottom_single] = Footer bottom single column



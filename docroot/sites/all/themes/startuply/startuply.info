name        = Startuply
description = A theme based on Radix.
screenshot  = screenshot.png
core        = 7.x

; Base - Radix
base theme  = radix

; Regions
regions[content] = Content

; Panels
plugins[panels][layouts] = panels/layouts
plugins[panels][styles] = panels/styles

; CSS - General
stylesheets[all][]     = assets/stylesheets/screen.css
stylesheets[print][]   = assets/stylesheets/print.css

; CSS - Conditional
stylesheets-conditional[lte IE 9][all][] = assets/stylesheets/ie.css
stylesheets-conditional[IE 8][all][] = assets/stylesheets/ie8.css

; Javascript
scripts[] = assets/javascripts/script.js

; Theme features
features[] = name
features[] = logo
features[] = favicon
features[] = main_menu
features[] = secondary_menu

; Signal to mvpcreator_theme that we support full width layouts.
settings[full_width] = 1

; Information added by Drupal.org packaging script on 2015-09-15
version = "7.x-1.x-dev"
core = "7.x"
project = "startuply"
datestamp = "1442334240"


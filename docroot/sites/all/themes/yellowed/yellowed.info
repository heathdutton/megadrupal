engine = phptemplate

; STYLESHEETS
;----------------------------------------------------
stylesheets[all][] = css/style.css
stylesheets[all][] = css/media.css

; SCRIPTS
;----------------------------------------------------
scripts[] = js/menu.js
scripts[] = js/flexslider-min.js
scripts[] = js/slide.js

; REGIONS
;----------------------------------------------------
regions[sidebar_first]  = First Sidebar
regions[sidebar_second] = Second Sidebar
regions[top_first]      = Header First
regions[top_second]     = Header Second
regions[top_third]      = Header Third
regions[content]        = Content
regions[content_top]    = Content Top
regions[footer_first]   = Bottom First
regions[footer_second]  = Bottom Second
regions[footer_third]   = Bottom Third
regions[footer]         = Footer

; FEATURES
;----------------------------------------------------
features[] = logo
features[] = name
features[] = slogan
features[] = node_user_picture
features[] = comment_user_picture
features[] = comment_user_verification
features[] = favicon
features[] = main_menu
features[] = secondary_menu


; name, description, screenshot, version - these are necessary for theme list

name        = "iDream D"
description = 'This a subtheme of OM Base Theme. - An adoption of the WordPress theme by <a title="author homepage" href="http://www.templatesnext.org/">marsian</a>'
screenshot  = "screenshot.png"

base theme = om

; compatibilities
core        = "7.x"
engine      = phptemplate

; default stylesheets

stylesheets[all][] = "css/style.css"

; jquery scripts
scripts[]       = "js/script.js"

; default regions, on the left inside the brackets 
; are the variable names used in page.tpl.php,
; on right side are the names necessary for blocks list

regions[search_box]     = Search Box
regions[highlighted]    = Highlighted
regions[content]        = Content
regions[sidebar_second] = "Sidebar - Second (Right)"

regions[footer_bottom_left] = Footer bottom column left
regions[footer_bottom_middle] = Footer bottom column middle
regions[footer_bottom_right] = Footer bottom column right
regions[footer_bottom_single] = Footer bottom single column





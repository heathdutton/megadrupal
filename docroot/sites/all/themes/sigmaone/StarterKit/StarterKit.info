name = SigmaOne StarterKit
description = <strong>SigmaOne Starter Kit</strong>, Starter Kit for Sigma One provided by <a href="http://www.victheme.com">VicTheme.com</a>.
core = 7.x
base theme = sigmaone

; Main default css
; Most likely you will need to override SigmaOne style.css
; stylesheets[all][] = css/style.css

; All search related styling should go here
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/searchpage.css

; All modification for node related area should go here
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/nodepage.css

; All comment related styling, including comment in forum
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/comment.css

; All block element related styling
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/block.css

; All generic styling related styling
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/generic.css

; All user page related styling
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/userpage.css

; All table related styling
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/table.css

; All forum related styling
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/forum.css

; All poll related styling
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/poll.css

; All navigation related styling
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/navigation.css

; Color css
; Uncomment to override SigmaOne CSS
; stylesheets[all][] = css/colors.css

; All user local modification for drupal standard element should go here
; stylesheets[all][] = css/local.css

; Load js script?
scripts[] = js/script.js

; Headers
regions[header_logo] = Logo
regions[header_right] = Header Right

; Sliders
regions[slider] = Slider

; Navigations
regions[navigation_left] = Navigation Left
regions[navigation_right] = Navigation Right

; Prefaces
regions[preface_left] = Preface Left
regions[preface_middle] = Preface Middle
regions[preface_right] = Preface Right

; Content
regions[content] = Content

; Region for Drupal 7 specific
regions[help] = Help
regions[page_top] = Page top
regions[page_bottom] = Page bottom

; Sidebars
regions[sidebar_first] = Sidebar First
regions[sidebar_second] = Sidebar Second

; Postfaces
regions[postface_left] = Postface Left
regions[postface_middle] = Postface Middle
regions[postface_right] = Postface Right

; Footers
regions[footer_left] = Footer Left
regions[footer_middle] = Footer Middle
regions[footer_right] = Footer Right

; Full Footers
regions[fullfooter] = Full Footer

features[] = main_menu
features[] = name
features[] = slogan
features[] = node_user_picture
features[] = comment_user_picture
features[] = logo
features[] = favicon

; enable shortcut menu link
settings[shortcut_module_link] = 1


